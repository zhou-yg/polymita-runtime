<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>

  <% css.forEach((path, i) => { %>
  <link rel="stylesheet" href="<%- css[i] %>">
  <% }) %>

  <link rel="stylesheet" href="/static/module.css" >
  
  <!-- deps -->
  <script src="/static/deps.js"></script>
  <script src="/static/runtime.js"></script>
  <!-- modules -->
  <script src="/static/module.js"></script>
  
  <script src="/static/dynamicConfig.js"></script>

  <% scripts.forEach((path, i) => { %>
  <script src="<%- scripts[i] %>"></script>
  <% }) %>
  
</head>
<body>
  <div id="app"></div>
</body>
<script type="module" src="<%= src %>"></script>
</html>
